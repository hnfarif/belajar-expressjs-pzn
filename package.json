{
  "name": "belajar-expressjs",
  "version": "1.0.0",
  "description": "Belajar ExpressJS",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "maxConcurrency": 2,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!vendor/**/*.{js,jsx}"
    ]
  },
  "author": "Hanif Arif",
  "license": "ISC",
  "dependencies": {
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "express": "^4.18.2",
    "cookie-parser": "^1.4.6",
    "mustache-express": "^1.3.2",
    "express-fileupload": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "babel-jest": "^29.6.1",
    "jest": "^29.0.0",
    "supertest": "^6.2.4"
  }
}
